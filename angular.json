{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false
  },
  "defaultProject": "data-binding",
  "newProjectRoot": "projects",
  "projects": {
    "data-binding": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                },
                {
                  "maximumError": "10kb",
                  "maximumWarning": "6kb",
                  "type": "anyComponentStyle"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "apps/abc/shared/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "apps/shared/assets",
                "output": "/assets/"
              }
            ],
            "extractCss": true,
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "dist/data-binding",
            "polyfills": "src/polyfills.ts",
            "scripts": [
            ],
            "styles": [
              "src/styles.scss",
              "apps/abc/shared/shared.scss",
              "apps/shared/shared.scss"
            ],
            "tsConfig": "tsconfig.app.json"
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "data-binding:serve:production"
            }
          },
          "options": {
            "devServerTarget": "data-binding:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "data-binding:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**"
            ],
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "data-binding:build:production"
            }
          },
          "options": {
            "browserTarget": "data-binding:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "apps/abc/shared/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "apps/shared/assets",
                "output": "/assets/"
              }
            ],
            "karmaConfig": "karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [
            ],
            "styles": [
              "src/styles.scss"
            ],
            "tsConfig": "tsconfig.spec.json"
          }
        }
      },
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "skipTests": true,
          "style": "scss"
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "sourceRoot": "src"
    }
  },
  "version": 1
}